			<z>-1.0</z>
		</position>
	</gear> -->

	<squeal>
		<name>squeal</name>
		<path>Sounds/squeal.wav</path>
		<condition>
			<or>
				<property>/gear/gear[0]/wow</property>
				<property>/gear/gear[1]/wow</property>
				<property>/gear/gear[2]/wow</property>
			</or>
		</condition>
		<volume>
			<property>/velocities/speed-down-fps</property>
			<factor>0.5</factor>
			<max>1.0</max>
		</volume>
		<volume>
			<property>/velocities/airspeed-kt</property>
			<factor>0.01</factor>
			<max>1.0</max>
		</volume>
		<volume>
			<internal>dt_stop</internal>
			<factor>0.05</factor>
			<max>1.0</max>
		</volume>
		<pitch>
			<property>/velocities/airspeed-kt</property>
			<factor>0.0025</factor>
			<offset>0.5</offset>
			<max>1.0</max>
		</pitch>
		<position>
			<x>2.0</x>
			<y>0.00001</y>
			<z>-1.0</z>
		</position>
	</squeal>
 
	<flap>
		<name>flaps</name>
		<mode>in-transit</mode>
		<path>Sounds/flaps.wav</path>
		<property>/surface-positions/flap-pos-norm</property>
		<volume>
			<factor>0.15</factor>
		</volume>
		<position>
			<x>2.5</x>
			<y>0.000001</y>
			<z>-1.0</z>
		</position>
	</flap>
  
	<canopy>
		<name>canopy-move</name>
		<mode>in-transit</mode>
		<path>Aircraft/Wuerger-Jsb/Sound/canopy-move.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>canopy/position-norm</property>
					<value>0.1</value>
				</greater-than>
				<less-than>
					<property>canopy/position-norm</property>
					<value>0.9</value>
				</less-than>
			</and>
		</condition>
		<volume>
			<factor>0.7</factor>
		</volume>
		<position>
			<x>1.5</x>
			<y>0.000001</y>
			<z>0.0</z>
		</position>
	</canopy>
  
	<canopy>
		<name>canopy-lock-open</name>
		<path>Aircraft/Wuerger-Jsb/Sound/canopy-lock.wav</path>
		<condition>   
			<greater-than>
				<property>canopy/position-norm</property>
				<value>0.9</value>
			</greater-than>
		</condition>
		<volume>
			<property>/fdm/jsbsim/simulation/soundOK</property>
			<factor>0.7</factor>
		</volume>
		<position>
			<x>1.5</x>
			<y>0.000001</y>
			<z>0.0</z>
		</position>
	</canopy>
  
	<canopy>
		<name>canopy-lock-closed</name>
		<path>Aircraft/Wuerger-Jsb/Sound/canopy-lock.wav</path>
		<condition>
			<less-than>
				<property>canopy/position-norm</property>
				<value>0.1</value>
			</less-than>
		</condition>
		<volume>
			<property>/fdm/jsbsim/simulation/soundOK</property>
			<factor>0.7</factor>
		</volume>
		<position>
			<x>1.5</x>
			<y>0.000001</y>
			<z>0.0</z>
		</position>
	</canopy>
  
	<wind>
		<name>wind</name>
		<mode>looped</mode>
		<path>Sounds/wind.wav</path>
		<property>/velocities/airspeed-kt</property>
		<volume>
			<property>/velocities/airspeed-kt</property>
			<factor>0.002</factor>
			<min>0.00</min>
			<max>0.25</max>
		</volume>
		<pitch>
			<property>/velocities/airspeed-kt</property>
			<factor>0.01</factor>
			<offset>0.5</offset>
			<max>1.0</max>
		</pitch>
		<position>
			<x>2.0</x>
			<y>0.000001</y>
			<z>1.0</z>
		</position>
	</wind>
  
	<wind-gear>
		<name>wind</name>
		<mode>looped</mode>
		<path>Sounds/wind-8khz.wav</path>
		<property>/gear/gear[0]/position-norm</property>
		<volume>
			<property>/velocities/airspeed-kt</property>
			<factor>0.002</factor>
			<min>0.00</min>
			<max>0.25</max>
