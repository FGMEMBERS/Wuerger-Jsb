<?xml version="1.0"?>
 
<PropertyList>

 <fx>

	<crank>
		<name>crank</name>
		<mode>looped</mode>
		<path>Aircraft/Wuerger-Jsb/Sound/starter.wav</path>
		<property>/engines/engine[0]/cranking</property>
		<volume>
			<offset>0.4</offset>
		</volume>
		<pitch>
			<property>engines/engine/rpm</property>
			<factor>0.0033</factor>
		</pitch>
		<position>
			<x>0.5</x>
			<y>0.00001</y>
			<z>-0.012</z>
		</position>
	</crank>

	<crank>
		<name>crank</name>
		<mode>looped</mode>
		<path>Aircraft/Wuerger-Jsb/Sound/cranking.wav</path>
		<condition>
			<property>/engines/engine[0]/cranking</property>
			<not>
				<property>/engines/engine[0]/running</property>
			</not>
		</condition>
		<volume>
			<offset>0.15</offset>
		</volume>
		<pitch>
			<property>/engines/engine/running</property>
			<factor>0.15</factor>
		</pitch>
		<position>
			<x>0.5</x>
			<y>0.000001</y>
			<z>-0.012</z>
		</position>
	</crank>

	<crank>
		<name>cough</name>
		<path>Aircraft/Wuerger-Jsb/Sound/coughing.wav</path>
		<property>/engines/engine[0]/running</property>
		<volume>
			<offset>0.3</offset>
		</volume>
		<reference-dist>100</reference-dist>
		<max-dist>1000</max-dist>
	</crank>

	<engine>
		<name>engine</name>
		<mode>looped</mode>
		<path>Aircraft/Wuerger-Jsb/Sound/running.wav</path>
		<property>/engines/engine[0]/running</property>
		<volume>
			<property>/engines/engine[0]/mp-osi</property>
			<factor>0.00375</factor>
			<min>0.15</min>
			<max>1.5</max>
		</volume>
		<volume>
			<property>/orientation/alpha-deg</property>
			<type>log</type>
			<min>1.0</min>
			<max>1.5</max>
		</volume>
		<volume>
			<property>/sim/current-view/internal</property>
			<offset>1.0</offset>
			<factor>-0.7</factor>
		</volume>
		<pitch>
			<property>/engines/engine[0]/rpm</property>
			<factor>0.0002</factor>
			<min>0.5</min>
			<max>2.0</max>
			<offset>0.15</offset>
		</pitch>
		<reference-dist>100</reference-dist>
		<max-dist>1000</max-dist>
	</engine>

	<propeller>
		<name>propeller</name>
		<mode>looped</mode>
		<path>Aircraft/Koenigsadler-Jsb/Sounds/propeller.wav</path>
		<property>/engines/engine[0]/running</property>
		<volume>
			<property>/orientation/alpha-deg</property>
			<type>log</type>
			<min>0.2</min>
			<max>2.0</max>
		</volume>
		<volume>
			<property>/sim/current-view/internal</property>
			<factor>0.5</factor>
		</volume>
		<pitch>
			<property>/engines/engine[0]/mp-osi</property>
			<factor>0.0036</factor>
			<min>0.5</min>
			<max>2.0</max>
			<offset>0.9</offset>
		</pitch>
		<reference-dist>100</reference-dist>
		<max-dist>1000</max-dist>
	</propeller>

	<rumble>
		<name>rumble</name>
		<mode>looped</mode>
		<path>Aircraft/Wuerger-Jsb/Sound/rumble.wav</path>
		<condition>
			<or>
				<property>/gear/gear[0]/wow</property>
				<property>/gear/gear[1]/wow</property>
				<property>/gear/gear[2]/wow</property>
			</or>
		</condition>
		<volume>
			<property>/velocities/airspeed-kt</property>
			<type>log</type>
			<factor>0.25</factor>
		</volume>
		<position>
			<x>2.0</x>
			<y>0.000001</y>
			<z>-1.0</z>
		</position>
	</rumble>

	<gear>
		<name>gear</name>
		<mode>in-transit</mode>
		<path>Sounds/gear.wav</path>
		<property>/gear/gear[0]/position-norm</property>
		<reference-dist>100</reference-dist>
		<max-dist>1000</max-dist>
	</gear>

	<gear>
		<name>gear-lock</name>
		<path>Sounds/gear-lck.wav</path>
		<condition>
			<greater-than>
				<property>/gear/gear[0]/position-norm</property>
				<value>0.9</value>
			</greater-than>
		</condition>
		<reference-dist>100</reference-dist>
		<max-dist>1000</max-dist>
	</gear>

